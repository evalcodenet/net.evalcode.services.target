{"http_connectors": [
  {
    "scheme": "HTTP",
    "host": "services.dev",
    "port": "8080",

    "enable_direct_buffers": true,
    "enable_reverse_lookup": false,
    "enable_statistics": true
  },

  {
    "scheme": "HTTPS",
    "host": "services.dev",
    "port": "8443",

    "keystore": "${net.evalcode.services.config}/default/keystore",
    "keystore_password": "services",
    "truststore": "${net.evalcode.services.config}/default/truststore",
    "truststore_password": "services",
    "certificate_alias": "services.dev",

    "enable_direct_buffers": true,
    "enable_reverse_lookup": false,
    "enable_statistics": true
  }
]}
