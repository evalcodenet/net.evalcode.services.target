ca {
  net.evalcode.services.ca.security.LdapLoginModule required
  host="auth.evalcode.net"
  port="389"
  baseUser="ou=Users,dc=evalcode,dc=net"
  baseGroup="ou=Groups,dc=evalcode,dc=net"
  objectClassUser="inetOrgPerson"
  objectClassGroup="groupOfUniqueNames"
  attributeName="cn"
  attributeMember="uniqueMember";
};

everwines {
  org.eclipse.jetty.plus.jaas.spi.PropertyFileLoginModule required
  file="/etc/net.evalcode/services/default.password";
};
